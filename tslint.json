{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "no-shadowed-variable": false,
    "member-access": false,
    "object-shorthand": ["error", "always"]
  }
}
